<!-- SUBDIRECTORY START -->
<!-- main-container start -->
<!-- ================ -->
<section class="main-container padding-bottom-clear">
	<div class="container">
		<!-- main start -->
		<!-- ================ -->
		<div class="main">
			<!-- page-title start -->
			<!-- ================ -->
			<h1 class="page-title"><strong>Research: </strong><span class="font-light">{{name}}</span></h1>
			{{> page/lead}}
			<!-- page-title end -->
		</div>
		<!-- main end -->
	</div>

	<div class="container">
		<div class="row grid-space-10">
			{{#if listings}}
				
				{{!-- Show projects --}}
				{{#each listings}}
					{{> sublisting listing=this}}
				{{/each}}

			{{else}}
				
				<strong>No projects! Fiddlesticks!</strong>
			
			{{/if}}
		</div>
	</div>
</section>
<!-- section end -->